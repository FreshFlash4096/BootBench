[global]
filename=${FILENAME}
randrepeat=0
time_based=1
runtime=${FIO_RUN_TIME}

[read_job]
rw=randread
bs=32k
thinktime=0
group_reporting=1
numjobs=${NUMJOBS}
direct=1
ioengine=libaio

[write_job]
rw=write
bs=128k
ioengine=sync
fsync=1024
rate=15m
rate_min=5m
rate_cycle=5000
rate_ignore_thinktime=1

[trim_job]
rw=randtrim
bs=128k
ioengine=sync
rate=5m
rate_min=2m
rate_cycle=5000
rate_ignore_thinktime=1
